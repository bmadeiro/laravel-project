{
    "name": "bmadeiro/laravel-project",
    "license": "MIT",
    "description": "Laravel New Project Generator",
    "keywords": [
        "laravel",
        "project",
        "generator",
        "laravel new project generator",
        "api generator"
    ],
    "authors": [
        {
            "name": "Bluecode"
        },
        {
            "name": "19peaches",
            "homepage": "https://github.com/19peaches"
        },
        {
            "name": "Bruno Madeiro",
            "email": "bmadeiro@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "laravel/framework": "5.6.*",
        "doctrine/dbal": "^2.5",
        "laracasts/flash": "dev-master",
        "laravelcollective/html": "^5.2"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.6",
        "phpunit/phpunit": "^5.7",
        "orchestra/testbench": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Bmadeiro\\LaravelProject\\": "src/"
        },
        "classmap": [
            "tests"
        ]
    },
    "suggest": {
        "doctrine/dbal": "Required to use generator from existing table."
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bmadeiro\\LaravelProject\\LaravelProjectServiceProvider"
            ]
        }
    }
}
